[begin] (BlackBox)
    [submenu] (Editor)
        [exec] (Nano) {lxterminal -e nano}
        [exec] (LXTerminal) {lxterminal}
        [exec] (PCManFM File Manager) {pcmanfm}
    [end]
    [submenu] (Multimedia)
        [exec] (PulseAudio Volume Control) {pavucontrol}
    [end]
    [submenu] (Internet)
        [exec] (Chromium) {chromium}
    [end]
    [submenu] (Sistema)
        [submenu] (Configuracion)
            [submenu] (Redes)
                [exec] (Configuracion de redes) {nm-connection-editor}
            [end]
            [exec] (FuzzyPKG) {fuzzypkg}
        [end]
        [exec] (Top) {lxterminal -e top}
    [end]
    [submenu] (BlackBox)
        [submenu] (Styles) {Estilos}
        [stylesdir] (/usr/share/blackbox/styles)
        [end]
        [submenu] (User Styles) {Estilos de Usuario}
        [stylesdir] (~/.blackbox/styles)
        [end]
        [workspaces] (Espacios)
        [config] (Configurar)
        [reconfig] (Reconfigurar)
        [restart] (Reinicar)
        [exit] (Salir)
    [end]
[end]
